{
  "name": "webmaster",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-sass": "node-sass --omit-source-map-url src/styles/styles.scss src/styles/styles.css",
    "watch-sass": "npm run build-sass -- --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@fullcalendar/core": "^4.4.0",
    "@fullcalendar/timegrid": "^4.4.0",
    "@fullcalendar/vue": "^4.4.0",
    "bootstrap": "^4.4.1",
    "core-js": "^3.3.2",
    "firebase": "^7.8.2",
    "jquery": "^3.5.0",
    "luxon": "^1.22.0",
    "node-sass": "^4.13.1",
    "popper.js": "^1.16.1",
    "vue": "^2.6.10",
    "vue-datetime": "^1.0.0-beta.11",
    "vue-router": "^3.1.3",
    "vue-select": "^3.4.0",
    "vuefire": "^2.2.1",
    "weekstart": "^1.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
